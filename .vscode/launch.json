{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome with Live Server",
      "type": "chrome",
      "request": "launch",
      "url": "http://127.0.0.1:5500/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "userDataDir": true
    },
    {
      "name": "Launch Production Site",
      "type": "chrome",
      "request": "launch",
      "url": "https://www.richyfesta.com",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    }
  ]
}
